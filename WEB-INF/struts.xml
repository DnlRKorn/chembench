       </action>
        
       <action name="error">
            <result>jsp/main/errors2.jsp</result>
       </action>
       
        <action name="loadRegistrationPage" class="edu.unc.ceccr.action.UserRegistrationAndProfileActions" method="loadUserRegistration">
            <result>jsp/administration/user/registrationForm.jsp</result>
        </action>
        
        <action name="registerUser" class="edu.unc.ceccr.action.UserRegistrationAndProfileActions" method="RegisterUser">
            <result name="success">jsp/administration/user/registrationApprovalMessage.jsp</result>
        </action>
        
        <action name="ceccrBase">
            <result name="success">jsp/main/underConstruction.jsp</result>
        </action>
        
        <!-- Free Software List page -->
        <action name="softwareList" class="edu.unc.ceccr.action.SoftwareLinksAction" method="loadPage">
            <result name="success">jsp/main/softwareLinks.jsp</result>
        </action>
        
        <action name="addSoftware" class="edu.unc.ceccr.action.SoftwareLinksAction" method="addSoftware">
